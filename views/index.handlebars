<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <meta http-equiv="X-UA-Compatible" content="ie=edge">
  <!-- Icon -->
  <link rel="icon" type="image/png" href="/img/AIDRLogo.png" />
  <!-- CSS files-->
  <link rel="stylesheet" href="/css/bootstrap.min.css">
  <link rel="stylesheet" href="/css/style.css">
  <title>AIDR - Web Application</title>
</head>

<body>
  <div class="wrapper">
    <!-- .......... Navbar .......... -->
    {{> _nav}}
    <!-- Page Content (Inactive at the beginning but wrote 'active' for jQuery (see below))  -->
    <div id="content" class="tab-content active">
      <!-- topbar -->
      {{> _topbar}}
      <!-- toast notifications -->
      <div id="toaster"></div>
      <!-- Dashboard -->
      <div class="tab-pane fade show active" id="nav-dash" role="tabpanel" aria-labelledby="nav-dash-tab">
        {{>_dashboard}}
      </div>
      <!-- Video and Control-->
      <div class="tab-pane fade" id="nav-video-control" role="tabpanel" aria-labelledby="nav-video-control-tab">
        {{>_videocontrol}}
      </div>
      <!-- Telemetry -->
      <div class="tab-pane fade" id="nav-telemetry" role="tabpanel" aria-labelledby="nav-telemetry-tab">
        {{>_telemetry}}
      </div>
      <!-- Mapping -->
      <div class="tab-pane fade" id="nav-mapping" role="tabpanel" aria-labelledby="nav-mapping-tab">
        {{>_mappinglocalization}}
      </div>
      <div class="line"></div>
        <!-- Footer -->
        {{>_footer}}
    </div>
  </div>
  <!-- .......... JS Scripts ..........-->
  <!-- Bootstrap JS files: jQuery first, then Popper.js, then Bootstrap JS -->
  <script src="/js/dependencies/jquery/jquery-3.3.1.slim.min.js" type="text/javascript"></script>
  <script src="/js/dependencies/popper/popper.min.js" type="text/javascript"></script>
  <script src="/js/dependencies/bootstrap/bootstrap4.2.1.min.js" type="text/javascript"></script>
  <!-- Font Awesome JS -->
  <script defer src="https://use.fontawesome.com/releases/v5.0.13/js/solid.js" integrity="sha384-tzzSw1/Vo+0N5UhStP3bvwWPq+uvzCMfrN1fEFe+xBmv1C/AtVX5K0uZtmcHitFZ" crossorigin="anonymous"></script>
  <script defer src="https://use.fontawesome.com/releases/v5.0.13/js/fontawesome.js" integrity="sha384-6OIrr52G08NpOFSZdxxz1xdNSndlD4vdcf/q2myIUVO0VsqaGHJsB0RaBE01VTOY" crossorigin="anonymous"></script>
  <!-- Robot Control Scripts-->
  <script src="/js/dependencies/ros/keyboardteleop4.js" type="text/javascript"></script>
  <!-- Canvasjs -->
  <script src="/js/dependencies/canvas/canvasjs.min.js" type="text/javascript"></script>
  <!-- Joystickjs -->
  <script src="/js/dependencies/joystick/joystickjs.js" type="text/javascript"></script>
  <!--Our own code-->
  <script src="/js/code/toaster/glorioustoaster.js" type="text/javascript"></script>
  <script src="/js/code/ros/rosfrontend.js" type="text/javascript"></script>
  <script src="/js/code/charts/canvasjsCode.js" type="text/javascript"></script>
  <!-- toaster code -->
  {{#if signedIn}}
    <script type="text/javascript">
      showSuccess('{{toastMessage}}');
    </script>
  {{/if}}
  <!-- Sidebar code -->
  <script type="text/javascript">
    $(document).ready(function() {
      $('#sidebarCollapse').on('click', function() {
        $('#sidebar').toggleClass('active');
        $('#content').toggleClass('active');
      });
    });
  </script>
  <!-- /Sidebar code -->
</body>
